{
  "name": "react-komposer",
  "version": "1.9.1",
  "description": "Compose React containers and feed data into components. (React 16)",
  "repository": {
    "type": "git",
    "url": "https://github.com/junlouiegonzales/react-komposer.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner lib/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": ". ./scripts/prepublish.sh",
    "lint": "eslint ./lib",
    "lintfix": "eslint ./lib --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch"
  },
  "react-native": "./dist/index.js",
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "4.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "browserify": "12.x.x",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "1.7.x",
    "eslint-plugin-babel": "2.x.x",
    "eslint-plugin-react": "3.x.x",
    "exposify": "0.5.x",
    "mocha": "^3.0.2",
    "nodemon": "1.7.x",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "rx": "4.x.x",
    "sinon": "^1.17.6",
    "uglifyify": "3.x.x"
  },
  "peerDependencies": {
    "react": "^0.14.3 || ^15.0.0 || ^16.0.0" 
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "hoist-non-react-statics": "^1.2.0",
    "invariant": "^2.2.4",
    "shallowequal": "^0.2.2"
  }
}
